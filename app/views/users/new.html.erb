<h1>New Users Page</h1>
<%= form_for @user do |form| %>
  <div>
    <%= form.label :username %>
    <%= form.text_field :username %><br>
    <% @user.errors.full_messages_for(:username).each do |message| %>
      <div style="color:red"><%= "Username has already taken" %></div>
    <% end %>
  </div>
  <div>
    <%= form.label :password %>
    <%= form.password_field :password %><br>
  </div>
  <%= form.submit "Create Account" %> 
<% end %>
